<h1 class='center-align'>{{tracks.0.artists.0.name}}</h1>


<ul class="collection with-header">
  <li class="collection-header">
    <div class='row'>
      <h4 class='col s6'>Title</h4>
      <h4 class='col s6 right-align'>Listen</h4>
    </div>

  </li>
  {{#each tracks}}
  <div class="collection-item  row">
    <li class="col s6">{{this.name}}</li>
    <li class="col s6 right-align">
      {{!-- <audio controls src={{this.preview_url}} muted></audio> --}}
      {{#if this.preview_url}}
      <button class="btn audio" src={{this.preview_url}}>Play</button>
      {{else}}
      <button class="btn audio" disabled>Play</button>

      {{/if}}
    </li>
  </div>
  {{/each}}
</ul>
<div class="void"></div>
<div id="waveform"></div>